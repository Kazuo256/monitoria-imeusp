<h1>Editando Secretária</h1>

<%= form_for :secretary, url: secretary_path(@secretary), method: :patch do |f| %>
  <p>
    <%= f.label :nusp, "Número USP" %><br>
    <%= f.text_field :nusp %>
  </p>

  <p>
    <%= f.label :name, "Nome" %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email, "Email" %><br>
    <%= f.email_field :email %>
  </p>

  <p>
    <%= f.label :password , "Senha" %><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit "Salvar"%>
  </p>
<% end %>

<%= link_to 'Voltar', secretaries_path %>
